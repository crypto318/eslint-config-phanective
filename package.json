{
  "name": "@phanect/eslint-plugin",
  "version": "2020.1.5",
  "description": "ESLint configuration for Jumpei Ogawa (@phanect)'s private projects",
  "main": "config.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ignore-pattern tests/ --ext=.js,.jsx,.ts,.tsx,.vue .",
    "clean": "git clean -dX --force",
    "release": "yarn publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-plugin.git"
  },
  "author": "Jumpei Ogawa",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-vue": "latest",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "typescript": "latest"
  },
  "devDependencies": {
    "eslint-plugin-jest": "latest",
    "jest": "^24.8.0"
  }
}
